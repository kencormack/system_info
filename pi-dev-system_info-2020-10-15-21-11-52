/dev/sdb -d sat -a -o on -S on -s (S/../.././03|L/../../7/03)
/dev/sda -a -d scsi
/dev/sdb -a -d sat
/dev/sda -a -d scsi -s L/../../3/18
/dev/sdb -a -d sat -s L/../../7/01

